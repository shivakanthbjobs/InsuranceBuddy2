{
	"invocation": "insurance buddy",
	"inputTypes": [
		{
			"name": "InsureAction",
			"values": [
				{
					"value": "policy"
				},
				{
					"value": "claim"
				},
				{
					"value": "policydetails"
				},
				{
					"value": "claimdetails"
				}
				
			]
		}
	],
	"alexa": {
		"interactionModel": {
			"languageModel": {
				"intents": [
					{
						"name": "AMAZON.YesIntent",
						"samples": [
							"Yes"
						]
					},
					{
						"name": "AMAZON.NoIntent",
						"samples": [
							"No"
						]
					},
					{
						"name": "AMAZON.CancelIntent",
						"samples": []
					},
					{
						"name": "AMAZON.HelpIntent",
						"samples": []
					},
					{
						"name": "AMAZON.StopIntent",
						"samples": []
					},
					{
						"name": "AMAZON.RepeatIntent",
						"samples": [
							"Repeat"
						]
					}
				]
			}
		}
	},
	"intents": [
		{
			"name": "StartIntent",
			"phrases": [
				"create {action}",
				"create a {action}",
				"fetch {action} "
			],
			"inputs": [
				{
					"name": "action",
					"type": "InsureAction"
				}
			]
		},

		{  
            "name":"FetchPolicyIntent",
			"phrases": [
				"my policy number is {policyno}",
				"policy number is {policyno}"
			],
			"inputs": [
				{
					"name": "policyno",
					"type": {
						"alexa": "AMAZON.NUMBER",
						"dialogflow": "@sys.any"
					}
				}
			]
        }

	],
	"dialogflow": {
		"intents": [
			{
				"name": "Default Fallback Intent",
				"auto": true,
				"webhookUsed": true,
				"fallbackIntent": true
			},
			{
				"name": "Default Welcome Intent",
				"auto": true,
				"webhookUsed": true,
				"events": [
					{
						"name": "WELCOME"
					}
				]
			}
		]
	}
}